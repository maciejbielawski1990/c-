<Window x:Class="Service_Management_Projekt.Logowanie"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Logowanie" Height="219" Width="153" ResizeMode="NoResize"
        Icon="Icon1.ico">
    <Grid>
        <Grid.Background>
            <LinearGradientBrush EndPoint="0.5,0.9" StartPoint="0.5,0.1">
                <GradientStop Color="#FFABDA0B" Offset="0"/>
                <GradientStop Color="#FFFFFFFF" Offset="1"/>
            </LinearGradientBrush>
        </Grid.Background>
        <TextBox Style="{StaticResource textbox}" Height="23" HorizontalAlignment="Left" Margin="4,36,0,0" Name="textBox1" VerticalAlignment="Top" Width="120" />
        <Label Content="Login" Height="28" HorizontalAlignment="Left" Margin="12,12,0,0" Name="label1" VerticalAlignment="Top" />
        <PasswordBox Style="{StaticResource passwordbox}" Height="23" HorizontalAlignment="Left" Margin="4,93,0,0" Name="passwordBox1" VerticalAlignment="Top" Width="120" />
        <Label Content="Password" Height="28" HorizontalAlignment="Left" Margin="12,65,0,0" Name="label2" VerticalAlignment="Top" />
        <Button Style="{StaticResource dbs}" Content="Log in" HorizontalAlignment="Left" Margin="27,129,0,0" Name="button1" VerticalAlignment="Top" Width="75" Click="button1_Click" />

    </Grid>
</Window>
